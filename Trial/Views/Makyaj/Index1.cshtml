@model List<Trial.Models.Urunler>
@{
    ViewBag.Title = "Index1";
}




<html>


<head>
    <style>
        * {
            padding: 0;
            margin: 0;
            box-sizing: border-box;
        }



        .topnav {
            overflow: hidden;
            background-color: #ea9d9d;
            width: 100%;
            height: 120px;
            padding: 0px;
        }

            .topnav a {
                margin-top: 2%;
                margin-left: 5%;
                float: left;
                display: block;
                color: #cf3030;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
                font-size: 20px;
                font-weight: 800;
            }

                .topnav a:hover {
                    background-color: #efb7b7;
                    color: #e97272;
                }

                .topnav a.active {
                    color: #cf3030;
                }

            .topnav .search-container {
                float: right;
            }

            .topnav input[type=text] {
                padding: 6px;
                margin-top: 8px;
                font-size: 12px;
                border: none;
                width: 60%;
                height: 25px;
            }

        .search-container {
            margin-top: 2%;
            height: 40px;
        }

        .topnav .search-container button {
            height: 25px;
            float: right;
            padding: 6px 10px;
            margin-top: 8px;
            margin-right: 22%;
            background: #efb7b7;
            font-size: 17px;
            border: none;
            cursor: pointer;
        }

            .topnav .search-container button:hover {
                background: #ccc;
            }

        #aralık {
            height: 120px;
        }

        .topnav input[type=text] {
            padding: 6px;
            margin-top: 8px;
            font-size: 17px;
            border: none;
        }


        ::-webkit-input-placeholder {
            font-size: 80%;
        }





        body {
            font-family: 'lato',sans-serif;
            font-size: 1.6rem;
            font-weight: 400;
            line-height: 1.5;
            background-color: #e9cbcb;
        }

        html {
            font-size: 10px;
        }

        .hizala {
            width: 50%;
            height: 400px;
            margin: 70px;
        }

        .form-group {
            padding-top: 25px;
        }

        #buton {
            height: 30px;
            width: 170px;
            background-color: #ea9d9d;
        }

        #urunler {
            height: 80px;
            width: 200px;
            float: left;
            margin-right: 40px;
            margin-top: 30px;
            background-color: #ea9d9d;
        }

            #urunler a {
                text-decoration: none;
                font-weight: 500;
                color: #cf3030;
                font-size: 22px;
            }

        #ayarla{
            margin-left:200px;
            margin-right:80px;
        }
        #mobile-nav {
            display: none;
            padding-left: 10px;
            margin-top: 0;
            width: 10%;
        }

        #top-bar {
            margin-left: 12px;
            margin-top: 25px;
            width: 100%;
            box-sizing: border-box;
            padding-left: 10px;
        }

        #mobile-nav li {
            margin-left: -4px;
            padding-top: 15px;
            list-style-type: none;
            width: 150px;
            display: block;
            border-bottom: thin solid #e9cbcb;
            border-top: none;
            width: 48px;
        }

            #mobile-nav li:hover {
                width: 110px;
                background-color: #e97272;
            }

            #mobile-nav li a {
                color: black;
                font-weight: 700;
                text-decoration: none;
                display: block;
                box-sizing: border-box;
                width: 80%;
                height: 100%;
            }

        #menu-button:hover {
            cursor: pointer;
        }


        /*.hizalam {
            float: left;
            height: 40px;
            width: 70%;
        }*/

        .hizalam a {
            font-size: 20px;
        }

        .container {
            margin-left: 2px;
            /*max-width: 250px;*/
        }
            #a {
                margin-top: 1%;
                margin-left: 2%;
                float: left;
                display: block;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
                font-size: 30px;
                font-weight: 700;
            }


    </style>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous" />
    <link href="https://fonts.googleapis.com/css?family=Poppins:400,700" rel="stylesheet">
    <link href="~/Css/StyleSheet1.css" rel="stylesheet" />
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <meta name="viewport" content="widht=device-width, initial-scale=1.0" />
    <meta name="description" content="" />
    <meta name="author" content="" />

</head>
<body>


    <div class="hizalam" style="margin-left:250px; margin-top:30px;">
        <a href="@Url.Action("anasayfa","Home",new { } )" class="smoothScroll" id="a" style="; margin-left:40px;margin-top:-15px; text-decoration:none;">Anasayfa</a>

        <a href="@Url.Action("Fotograflar","Home",new { })" class="smoothScroll" id="a" style=" margin-left:10px;margin-top:-15px; text-decoration:none;">Kombinler</a>
        <a href="@Url.Action("denem","Markalar",new { } )" id="a" class="baslık" style=" margin-left:40px; text-decoration:none; margin-top:-14px;">Markalar</a>
        <a href="@Url.Action("Register","Member",new {} )" class="smoothScroll" id="a" style=" margin-left:40px;margin-top:-15px;text-decoration:none;">Kaydol</a>
    </div>



    <div id="aralık"></div>
    <div id="ayarla">

        @foreach (var ulke in Model)
        {


            <figcaption>
                <a class="view_btn" href="/Makyaj/detay1/@ulke.UrunId"></a>
            </figcaption>
            <div class="text">
                <button id="urunler">
                    <h5><a href="/Makyaj/Detay1/@ulke.UrunId">@ulke.UrunAdi</a></h5>
                </button>
            </div>

        }

    </div>


</body>
</html>

